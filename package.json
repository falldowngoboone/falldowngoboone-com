{
  "name": "falldowngoboone-com",
  "version": "0.1.0",
  "description": "Built on top of the Eleventy static site generator.",
  "scripts": {
    "build": "npm-run-all clean build:*",
    "build:css": "sass ./src/sass/:./_site/css/",
    "build:html": "eleventy --input=src",
    "build:webpack": "webpack --env.production",
    "clean": "rimraf _site",
    "debug": "DEBUG=* npm run build:html",
    "watch": "run-p watch:*",
    "watch:css": "sass --watch ./src/sass/:./_site/css/",
    "watch:html": "eleventy --serve --input=src",
    "watch:webpack": "webpack",
    "start": "npm run watch"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/falldowngoboone/falldowngoboone-com.git"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.11.0",
    "@11ty/eleventy-navigation": "^0.1.5",
    "@11ty/eleventy-plugin-rss": "^1.0.7",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.0.1",
    "dotenv": "^8.2.0",
    "liquidjs": "^9.12.0",
    "luxon": "^1.24.1",
    "markdown-it": "^11.0.0",
    "markdown-it-anchor": "^5.3.0",
    "mkdirp": "^1.0.4",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "sass": "^1.26.7",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  }
}
